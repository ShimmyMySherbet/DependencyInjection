<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net48;net5</TargetFrameworks>
	<Nullable>enable</Nullable>
	<WarningsAsErrors>nullable</WarningsAsErrors>
	<LangVersion>9.0</LangVersion>
	<PackageId>ShimmyMySherbet.DependencyInjection</PackageId>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageTags>Dependancy Injection</PackageTags>
	<NeutralLanguage>English</NeutralLanguage>
	<Authors>ShimmyMySherbet</Authors>
	<Description>A light-weight and non-strict dependency injection container.
Allows for services to be registered at runtime, facilitating plugin systems and dynamic services.</Description>
	<PackageProjectUrl></PackageProjectUrl>
	<RepositoryUrl>https://github.com/ShimmyMySherbet/DependencyInjection</RepositoryUrl>
	<AssemblyName>ShimmyMySherbet.DependencyInjection</AssemblyName>
	<RootNamespace>ShimmyMySherbet.DependencyInjection</RootNamespace>
	<Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="6.0.1" />
  </ItemGroup>

</Project>
